{
  "version": "0.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "@angular/cdk": ">=8.0.0",
    "@storybook/instrumenter": ">=6.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@marklb/storybook-harness",
  "author": "Mark Berry (markberry867@gmail.com)",
  "module": "dist/storybook-harness.esm.js",
  "size-limit": [
    {
      "path": "dist/storybook-harness.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/storybook-harness.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.3",
    "husky": "^7.0.4",
    "size-limit": "^7.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "@angular/cdk": "^13.0.2",
    "@storybook/instrumenter": "^6.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
